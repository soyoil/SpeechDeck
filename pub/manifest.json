{
  "manifest_version": 2,
  "name": "SpeechDeck",
  "version": "0.1.0",

  "content_scripts": [
      {
          "matches": [
              "https://tweetdeck.twitter.com/"
          ],
          "js": [
              "content.js"
          ]
      }
  ],

  "browser_action": {
      "default_popup": "popup.html"
  },

  "permissions": [
      "activeTab"
  ]
}